{
	"name": "lesjoursfr/filesystem-cache",
	"type": "library",
	"description": "A PSR-6 cache implementation using filesystem. This implementation supports tags",
	"keywords": [
		"cache",
		"psr-6",
		"filesystem",
		"tag"
	],
	"homepage": "https://github.com/lesjoursfr/php-filesystem-cache",
	"license": "MIT",
	"authors": [
		{
			"name": "Adrien ERAUD",
			"email": "a.eraud@lesjours.fr",
			"homepage": "https://github.com/e-adrien",
			"role": "Developer"
		}
	],
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"require": {
		"php": "^8.0",
		"psr/cache": "^3.0",
		"psr/log": "^3.0.2",
		"psr/simple-cache": "^3.0"
	},
	"require-dev": {
		"escapestudios/symfony2-coding-standard": "^3.15.0",
		"phpunit/phpunit": "^11.5.4",
		"phpstan/phpstan": "^2.1.2",
		"phpstan/extension-installer": "^1.4.3",
		"phpstan/phpstan-phpunit": "^2.0.4",
		"phpstan/phpstan-deprecation-rules": "^2.0.1",
		"squizlabs/php_codesniffer": "^3.11.3"
	},
	"provide": {
		"psr/cache-implementation": "^3.0",
		"psr/simple-cache-implementation": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"FileSystemCache\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FileSystemCache\\Tests\\": "tests/"
		}
	},
	"archive": {
		"exclude": [
			".github",
			".vscode",
			".editorconfig",
			".gitignore",
			".php-cs-fixer.php",
			".whitesource",
			"vendor",
			"composer.lock",
			"composer.phar",
			"phpcs.xml",
			"phpstan.neon",
			"phpunit.xml.dist",
			".phpunit.cache"
		]
	}
}
